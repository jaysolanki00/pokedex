{
  "name": "pokedex",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prod_sw_build": "npm run build && node workbox-inject.js",
    "build_prod": "npm run build && browserify -s main dist/pokedex/runtime**js dist/pokedex/es2015**js dist/pokedex/polyfills**js dist/pokedex/main**js > dist/pokedex/bundle.js && npm run minify && node workbox-inject.js",
    "minify": "uglifyjs dist/pokedex/bundle.js --screw-ie8 --compress --mangle --output dist/pokedex/bundle.min.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/pwa": "^0.12.4",
    "@angular/router": "~7.2.0",
    "@angular/service-worker": "~7.2.0",
    "bootstrap": "^4.4.1",
    "core-js": "^2.5.4",
    "ngx-infinite-scroll": "^8.0.2",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "uglify-js": "^3.9.2",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.6",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "browserify": "^16.5.1",
    "codelyzer": "~4.5.0",
    "firebase-tools": "^8.2.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2",
    "workbox-build": "^5.0.0"
  }
}
